{
  "consumer": {
    "name": "react-client"
  },
  "provider": {
    "name": "express-server"
  },
  "interactions": [
    {
      "description": "requesting all the fruits",
      "request": {
        "method": "GET",
        "path": "/api/v1/fruits",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "fruits": [
            {
              "id": 1,
              "name": "apple",
              "calories": 100
            }
          ]
        },
        "matchingRules": {
          "$.body.fruits": {
            "min": 1
          },
          "$.body.fruits[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}